{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "7.4.*",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "doctrine/mongodb-odm": "2.0.0",
        "doctrine/mongodb-odm-bundle": "^4.3",
        "doctrine/reflection": "^1.2",
        "gedmo/doctrine-extensions": "3.0",
        "knplabs/knp-paginator-bundle": "5.4.2",
        "mongodb/mongodb": "^1.8",
        "predis/predis": "^1.1",
        "sensio/framework-extra-bundle": "6.0",
        "stof/doctrine-extensions-bundle": "^1.5",
        "symfony/asset": "4.2.*",
        "symfony/browser-kit": "4.4.*",
        "symfony/cache": "4.4.0",
        "symfony/dotenv": "5.2.*",
        "symfony/expression-language": "4.2.*",
        "symfony/flex": "1.*",
        "symfony/form": "4.*",
        "symfony/framework-bundle": "4.4",
        "symfony/http-client": "4.4.*",
        "symfony/mailer": "4.4.*",
        "symfony/messenger": "4.4.*",
        "symfony/monolog-bundle": "3.6.*",
        "symfony/orm-pack": "*",
        "symfony/postmark-mailer": "4.4.*",
        "symfony/process": "4.2.*",
        "symfony/proxy-manager-bridge": "4.2.*",
        "symfony/security-bundle": "4.4",
        "symfony/security-csrf": "*",
        "symfony/sendgrid-mailer": "4.4.*",
        "symfony/serializer-pack": "*",
        "symfony/translation": "4.4.0",
        "symfony/twig-bundle": "4.4",
        "symfony/validator": "4.*",
        "symfony/web-link": "4.2.*",
        "symfony/web-profiler-bundle": "*",
        "symfony/yaml": "4.2.*",
        "twig/extra-bundle": "3.x-dev",
        "twig/markdown-extra": "3.3.*"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "3.1.0",
        "symfony/config": "^4.4",
        "symfony/css-selector": "4.4.*",
        "symfony/debug-pack": "*",
        "symfony/maker-bundle": "1.0.0",
        "symfony/profiler-pack": "*",
        "symfony/test-pack": "*",
        "symfony/web-server-bundle": "4.2.*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.4.*"
        }
    }
}
